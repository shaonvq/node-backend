    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "1.1.2",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
